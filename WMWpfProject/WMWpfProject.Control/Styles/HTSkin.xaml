<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  全白 透明色  -->
    <SolidColorBrush x:Key="TransparentColor"
                     Color="#00FFFFFF" />
    <!--  全黑 透明色  -->
    <SolidColorBrush x:Key="TransparentBlackColor"
                     Color="#00000000" />
    <!--  线条颜色  -->
    <SolidColorBrush x:Key="LineColor"
                     Color="#22777777" />
    <!--  主题色  -->
    <SolidColorBrush x:Key="ThemeColor"
                     Color="#2196F3" />
    </ResourceDictionary>